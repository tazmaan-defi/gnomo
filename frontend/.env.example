# Gnomo DEX Frontend Configuration
# Copy this file to .env.local and update values for your environment

# =============================================================================
# Network Configuration
# =============================================================================

# RPC endpoint for Gno blockchain
# Local dev: http://127.0.0.1:26657
# Testnet: https://rpc.test5.gno.land:443
NEXT_PUBLIC_RPC_URL=http://127.0.0.1:26657

# Chain ID for wallet connection
# Local dev: dev
# Testnet: test5
NEXT_PUBLIC_CHAIN_ID=dev

# Chain name displayed in wallet
NEXT_PUBLIC_CHAIN_NAME=Gno Dev Local

# =============================================================================
# Contract Package Paths
# =============================================================================

# V2 AMM contract path
# Local dev: gno.land/r/dev/gnomo
# Testnet: gno.land/r/gnomo/dex (or your deployed path)
NEXT_PUBLIC_GNOMO_PKG_PATH=gno.land/r/dev/gnomo

# CLMM contract path
# Local dev: gno.land/r/dev/clmm
# Testnet: gno.land/r/gnomo/clmm (or your deployed path)
NEXT_PUBLIC_CLMM_PKG_PATH=gno.land/r/dev/clmm
